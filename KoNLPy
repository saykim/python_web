from konlpy.tag import Komoran
print(komoran.morphs(u'우왕 코모란도 오픈소스가 되었네요'))
print(komoran.nouns(u'오픈소스에 관심이 무척많은 멋진개발자들'))
print(komoran.pos(u'한글형태소분석기 코모란 테스트 중중중 입니다'))

###Result###
['우왕', '코', '모란', '도', '오픈', '소스', '가', '되', '었', '네요']
['오픈', '소스', '관심', '개발자']
[('한글', 'NNP'), ('형태소', 'NNP'), ('분석기', 'NNG'), ('코모', 'NNP'), ('란', 'JX'), 
  ('테스트', 'NNP'), ('중', 'NNB'), ('중', 'NNP'), ('중', 'NNB'), ('이', 'VV'), ('ㅂ니다', 'EC')]
